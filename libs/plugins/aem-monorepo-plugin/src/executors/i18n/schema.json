{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "properties": {
    "appPath": {
      "type": "string",
      "description": "Path to the mfe app."
    },
    "prefixes": {
      "type": "string",
      "description": "Comma-separated list of prefixes to filter i18n keys."
    }
  },
  "required": ["appPath", "prefixes"]
}
