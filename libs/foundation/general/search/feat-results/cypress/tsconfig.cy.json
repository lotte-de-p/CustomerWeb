{
  "extends": "../tsconfig.spec.json",
  "compilerOptions": {
    "outDir": "../../../../../../dist/out-tsc",
    "strict": true,
    "types": ["node", "global"],
    "preserveSymlinks": true,
    "sourceMap": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "inlineSourceMap": true
  },
  "exclude": [],
  "include": [
    "support/**/*.ts",
    "../cypress.config.ts",
    "../**/*.cy.ts",
    "../**/*.cy.tsx",
    "../**/*.cy.js",
    "../**/*.cy.jsx",
    "../**/*.d.ts"
  ]
}
